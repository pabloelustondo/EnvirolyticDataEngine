@model JassWeather.Controllers.BlobController.ShowDashBoard4DayViewModel
@using JassWeather.Models



@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>JassBuilder</legend>
        <div class="editor-label">
            @Html.LabelFor(model => model.fileName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.fileName)
            @Html.ValidationMessageFor(model => model.fileName)
        </div>
        <div class="editor-field">
            @Html.DisplayFor(model => model.schema)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.JassGridID, "JassGrid")
        </div>
        <div class="editor-field">
            @Html.DropDownList("JassGridID", String.Empty)
            @Html.ValidationMessageFor(model => model.JassGridID)
        </div>

         <p>
            <input type="submit" value="Submit" />
        </p>
     </fieldset>
}

