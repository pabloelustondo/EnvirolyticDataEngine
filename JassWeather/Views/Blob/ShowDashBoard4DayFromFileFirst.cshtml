@model JassWeather.Models.JassWeatherAPI.VariableValueModel



@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>JassBuilder</legend>
        <div class="editor-label">
            FileName
        </div>
        <div class="editor-field">
            @Html.TextAreaFor(model => model.fileName)
            @Html.ValidationMessageFor(model => model.fileName)
        </div>
        <div class="editor-field">
            @Html.DisplayFor(model => model.message)
        </div>
        <div class="editor-field">
            @Html.DisplayFor(model => model.schema)
        </div>
        <div class="editor-field">
            @Html.DisplayFor(model => model.generalMetadata)
        </div>
        <div class="editor-field">
            EnvirolyticFile: @Html.DisplayFor(model => model.envirolyticFile)
        </div>
        <div class="editor-field">
            Day Range: FROM @Html.DisplayFor(model => model.startingDate) TO  @Html.DisplayFor(model => model.endingDate)  
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.JassGridID, "JassGrid")
        </div>
        <div class="editor-field">
            @Html.DropDownList("JassGridID", String.Empty)
            @Html.ValidationMessageFor(model => model.JassGridID)
        </div>

        <div class="editor-label">
            Year
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.year)
            @Html.ValidationMessageFor(model => model.year)
        </div>
                <div class="editor-label">
            Month
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.monthIndex)
            @Html.ValidationMessageFor(model => model.monthIndex)
        </div>
                <div class="editor-label">
            @Html.LabelFor(model => model.dayIndex)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.dayIndex)
            @Html.ValidationMessageFor(model => model.dayIndex)
        </div>


         <p>
            <input type="submit" value="Submit" />
        </p>
     </fieldset>
}

